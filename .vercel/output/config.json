{
	"version": 3,
	"routes": [
		{
			"handle": "filesystem"
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"src": "^/_server-islands/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/_image/?$",
			"dest": "_render"
		},
		{
			"src": "^/admin/inbox/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/admin/message/update/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/message/reply/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/message/?$",
			"dest": "_render"
		},
		{
			"src": "^/fragments/?$",
			"dest": "_render"
		},
		{
			"src": "^/letters/?$",
			"dest": "_render"
		},
		{
			"src": "^/message/sent/?$",
			"dest": "_render"
		},
		{
			"src": "^/message/?$",
			"dest": "_render"
		},
		{
			"src": "^/writings/?$",
			"dest": "_render"
		},
		{
			"src": "^/$",
			"dest": "_render"
		}
	]
}